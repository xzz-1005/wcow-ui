{
  "name": "wcow-ui",
  "private": false,
  "version": "0.2.2",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview"
  },
  "exports": {
    ".": {
      "import": "./dist/wcow-ui.es.js",
      "require": "./dist/wcow-ui.umd.js",
      "types": "./dist/wcow-ui.d.ts"
    },
    "./dist/*": {
      "import": "./dist/wcow-ui.es.js",
      "require": "./dist/wcow-ui.umd.js",
      "types": "./dist/wcow-ui.d.ts"
    }
  },
  "main": "./dist/wcow-ui.umd.js",
  "module": "./dist/wcow-ui.es.js",
  "types": "./dist/wcow-ui.d.ts",
  "sideEffects": [
    "css",
    "dist/*",
    "css/*.css",
    "es/components/*/style.mjs",
    "lib/components/*/style.js",
    "style/**/*.scss",
    "theme/**/*.css"
  ],
  "files": [
    "dist"
  ],
  "license": "MIT",
  "keywords": [
    "vue",
    "vite"
  ],
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "autoprefixer": "^10.4.7",
    "postcss-import": "^14.1.0",
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^3.0.0",
    "sass": "^1.53.0",
    "typescript": "^4.6.4",
    "vite": "^3.0.0",
    "vite-plugin-dts": "^1.4.0"
  }
}