{
  "name": "wcow-ui",
  "private": false,
  "version": "0.2.54",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview"
  },
  "main": "lib/index.js",
  "module": "es/index.mjs",
  "types": "es/index.d.ts",
  "sideEffects": [
    "css",
    "dist/*",
    "css/*.css",
    "es/components/*/style.mjs",
    "lib/components/*/style.js",
    "style/**/*.scss",
    "theme/**/*.css"
  ],
  "exports": {
    ".": {
      "import": "./es/components/index.mjs",
      "require": "./lib/components/index.js"
    },
    "./es": "./es/components/index.mjs",
    "./lib": "./lib/components/index.js",
    "./es/*.mjs": "./es/components/*/index.mjs",
    "./es/*": "./es/components/*/index.mjs",
    "./lib/*.js": "./lib/components/*/index.js",
    "./lib/*": "./lib/components/*/index.js",
    "./package.json": "./package.json",
    "./*": "./*"
  },
  "files": [
    "dist",
    "es",
    "lib"
  ],
  "license": "MIT",
  "keywords": [
    "vue",
    "vite"
  ],
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "autoprefixer": "^10.4.7",
    "fast-glob": "^3.2.11",
    "postcss-import": "^14.1.0",
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^3.0.0",
    "sass": "^1.53.0",
    "typescript": "^4.6.4",
    "vite": "^3.0.0",
    "vite-plugin-dts": "^1.4.0"
  }
}